{
  "name": "dynamic-nft",
  "version": "0.0.1",
  "description": "A Chainlink example in a Truffle box",
  "scripts": {
    "compile": "truffle compile",
    "migrate": "truffle migrate --reset --network rinkeby",
    "verify": "truffle run verify DnDc --network rinkeby --license MIT",
    "fund": "truffle exec scripts/fund-contract.js --network rinkeby",
    "new": "truffle exec scripts/newChar.js --network rinkeby",
    "clean": "rm -rf build node_modules"
  },
  "license": "MIT",
  "dependencies": {
    "@chainlink/contracts": "^0.1.6",
    "@openzeppelin/contracts": "^3.4.1",
    "truffle-plugin-verify": "^0.5.7",
    "dotenv": "^8.2.0",
    "truffle": "^5.1.65"
  },
  "devDependencies": {
  }
}
